//output:index.html

doctype html
html(lang="en")
  head
    title DevTips Starter Kit
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="shortcut icon", href="assets/img/favicons/favicon.ico")
    link(rel="stylesheet", href="style.css")

  body
    .page-home
      section
        h1.page-title DevTips <br/>
          |Starter Kit
        p Ground zero for your next project
        
        a(href="https://github.com/DevTips/DevTips-Starter-Kit/archive/master.zip", class="button button-download") Download now
        
        a(class="fork-tag" href="https://github.com/DevTips/DevTips-Starter-Kit") Fork Me on GitHub
        
      section
        article
          h4 Watch the video
          .flex-video-wrap
            .flex-video
              iframe(src="https://www.youtube.com/embed/F3u7whqIxrc?rel=0&amp;controls=0&amp;showinfo=0", frameborder="0", allowfullscreen)
              
        article
          p Brought to you by
          .name-lockup
            img(class="avatar", src="https://yt3.ggpht.com/-47U43a3eNW0/AAAAAAAAAAI/AAAAAAAAAAA/fWgb7mKcn6k/s176-c-k-no/photo.jpg")
            .name
              strong Travis Neilson
              br
              a(href="https://twitter.com/DevTipsShow") @DevTipsShow
        
